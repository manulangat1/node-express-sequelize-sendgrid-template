{
  "name": "express-template",
  "version": "1.0.0",
  "description": "This is a template for node,express,sequelize and sendgrid projects",
  "main": "server.js",
  "scripts": {
    "server": "nodemon --exec babel-node ./server/server.js",
    "test": "nyc --reporter=lcov --reporter=text mocha --require  @babel/register 'server/**/**test.js' --exit",
    "coverage": "nyc npm run test && nyc report --reporter=text-lcov --reporter=lcov | node ./node_modules/coveralls/bin/coveralls.js --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manulangat1/node-express-sequelize-sendgrid-template.git"
  },
  "keywords": [
    "Node",
    "Express",
    "Postgresql",
    "Sequelize",
    "Sendgrid"
  ],
  "author": "Emmanuel Kipchirchir Langat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/manulangat1/node-express-sequelize-sendgrid-template/issues"
  },
  "homepage": "https://github.com/manulangat1/node-express-sequelize-sendgrid-template#readme",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^8.2.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0",
    "sequelize-test-helpers": "^1.3.2",
    "sinon": "^9.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0"
  }
}
